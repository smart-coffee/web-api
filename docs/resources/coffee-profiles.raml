securedBy: [ jwt ]
get:
  description: Liefert eine Liste aller Kaffee-Profile.
  responses: 
    200:
      description: Ok.
      body:
        type: coffee-profile[]
post:
  description: Erstellt ein Kaffee-Profil.
  body:
    type: coffee-profile-create
  responses: 
    201:
      description: Created. Kaffee-Profil erstellt.
      headers:
        Location:
          description: URL zum neu angelegten Kaffee-Profil.
      body:
        type: coffee-profile
/{id}:
  securedBy: [ jwt ]
  uriParameters: 
    id: 
      description: ID des Kaffee-Profils.
      type: integer
      example: 42
  get:
    description: Liefert das Kaffee-Profil mit der angegebenen ID.
    responses: 
      200:
        description: Ok.
        body:
          type: coffee-profile
  put:
    description: Bearbeitet das Kaffee-Profil mit der angegebenen ID.
    body:
      type: coffee-profile-edit
    responses: 
      200:
        description: Ok. Kaffee-Profil bearbeitet.
        body:
          type: coffee-profile
  delete:
    description: Löscht das Kaffee-Profil mit der angegebenen ID.
    responses: 
      204:
        description: No Content. Kaffee-Profil gelöscht.
        body:
          type: coffee-profile